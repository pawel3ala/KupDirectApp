{
  "expo": {
    "name": "KupDirectApp",
    "slug": "KupDirectApp",
    "privacy": "public",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "2.0.0",
    "orientation": "landscape",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#f08032"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "direct.kup.kupdirectapp",
      "buildNumber": "2",
      "infoPlist": {
        "NSCameraUsageDescription": "Potrzebny jest dostep do Twojego aparatu aby móc zeskanowac dowód rejestracyjny"
      }
    },
    "android": {
      "package": "com.kupdirectapp",
      "versionCode": 2,
      "permissions": ["CAMERA"]
    },
    "hooks": {
      "postPublish": [
        {
          "file": "sentry-expo/upload-sourcemaps",
          "config": {
            "organization": "your team short name here",
            "project": "your project short name here",
            "authToken": "your auth token here"
          }
        }
      ]
    }
  }
}
